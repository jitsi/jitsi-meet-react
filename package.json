    "react": "15.2.1",
    "react-dom": "15.2.0",
    "react-fontawesome": "^1.1.0",
    "react-native": "0.29.2",
    "react-native-vector-icons": "^2.0.3",
    "react-native-webrtc": "jitsi/react-native-webrtc#lib-jitsi-meet",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
